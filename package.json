{
  "name": "umi-locale-doctor",
  "version": "1.0.0",
  "description": "A doctor checks which keys are miss defined in locales, and which keys are never used in source code",
  "main": "./bin/index.js",
  "bin": {
    "udoctor": "./bin/index.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "test": "jest",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "build": "rollup -c",
    "prepublish": "rm -rf ./bin && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umijs/umi-locale-doctor.git"
  },
  "keywords": [
    "doctor",
    "locales"
  ],
  "files": [
    "bin"
  ],
  "author": "Howard.Zuo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umijs/umi-locale-doctor/issues"
  },
  "homepage": "https://github.com/umijs/umi-locale-doctor#readme",
  "dependencies": {
    "@babel/parser": "^7.6.2",
    "@babel/traverse": "^7.6.2",
    "@babel/types": "^7.6.1",
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "glob": "^7.1.4",
    "mz": "^2.7.0",
    "ora": "^4.0.2",
    "table": "^5.4.6",
    "tsconfig-paths": "^3.9.0",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/babel__traverse": "^7.0.7",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.18",
    "@types/mz": "0.0.32",
    "@types/node": "^12.7.8",
    "@types/table": "^4.0.7",
    "coveralls": "^3.0.6",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "rollup": "^1.22.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.6.3"
  }
}
